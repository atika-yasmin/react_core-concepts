[{"C:\\Web-development\\react_core-concepts\\src\\App.js":"1"},{"size":2239,"mtime":1606566830571,"results":"2","hashOfConfig":"3"},{"filePath":"4","messages":"5","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"6"},"1vg7dy","C:\\Web-development\\react_core-concepts\\src\\App.js",["7"],"import React, {useEffect, useState} from 'react';\nimport logo from './logo.svg';\nimport './App.css';\n\nfunction App() {\n  const heros = ['Ayan','Ahaan','Shiddhartha','Rayan','Adittiya']\n  const products = [\n    {name: 'Photoshop', price: '$90.99'},\n     {name: 'PDF Reader', price: '$6.90'},\n     {name: 'Illastrator', price: '$60.90'}        \n  ]\n  return (\n    <div className=\"App\">\n      <header className=\"App-header\">\n        <p>Arika</p>\n        <Users></Users>\n        <Counter></Counter>\n        <ul>\n          {\n            heros.map(hero => <li>{hero}</li>)\n          }\n          {\n            products.map(product => <li>{product.name}</li>)\n          }\n        </ul>\n        {\n          products.map(pd => <Product product={pd}></Product>)\n        }\n        <Person name=\"Ayan\" job=\"Hero\"></Person>\n        <Person name=\"Sakib\" job=\"Cricketer\"></Person>\n      </header>\n    </div>\n  );\n}\n\nfunction Product(props){\n  const productStyle={\n    border:'2px solid cyan',\n    borderRadius:'5px',\n    color:'blue',\n    backgroundColor:'lightgrey',\n    height:'200px',\n    width:'200px',\n    float:'left'\n  }\n  const {name, price} = props.product;\n  return(\n    <div style={productStyle}>\n        <h3>{name}</h3>\n        <h5>{price}</h5>\n        <button>Buy Now</button>\n    </div>\n  )\n}\n\nfunction Counter(){\n  const [count, setCount] = useState(25);\n  return (\n    <div>\n      <h2>Count: {count}</h2>\n      <button onMouseMove= {() => setCount(count + 1)}>Increase</button>\n      <button onClick={() => setCount(count - 1)}>Decrease</button>\n    </div>\n  )\n}\nfunction Users(){\n  const[users, setUsers] = useState([]);\n  useEffect(()=>{\n    fetch('https://jsonplaceholder.typicode.com/users')\n    .then(res => res.json())\n    .then(data => setUsers(data));\n  }, [])\n\n  return (\n    <div>\n      <h3>Dynamic User: {users.length}</h3>\n      <ul>\n        {\n          users.map(user => <li>{user.phone}</li>)\n        }\n        {\n          users.map(user => <li>{user.name}</li>)\n        }\n        {\n          users.map(user => <li>{user.email}</li>)\n        }\n      </ul>\n    </div>\n  )\n}\nfunction Person(props){\n  return (\n    <div>\n      <h3>Name: {props.name}</h3>\n      <p>Profession: {props.job}</p>\n    </div>\n  )\n}\nexport default App;",{"ruleId":"8","severity":1,"message":"9","line":2,"column":8,"nodeType":"10","messageId":"11","endLine":2,"endColumn":12},"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar"]